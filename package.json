{
  "name": "@nodenv/nodenv-nvmrc",
  "version": "1.0.6",
  "description": "A nodenv plugin to activate Node version from .nvmrc.",
  "homepage": "https://github.com/nodenv/nodenv-nvmrc#readme",
  "license": "MIT",
  "author": "Nan Wu",
  "contibutors": [
    "Jason Karns <jason.karns@gmail.com> (http://jasonkarns.com)"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodenv/nodenv-nvmrc.git"
  },
  "bugs": {
    "url": "https://github.com/nodenv/nodenv-nvmrc/issues"
  },
  "directories": {
    "bin": "bin",
    "test": "test"
  },
  "files": [
    "bin",
    "etc"
  ],
  "scripts": {
    "start": "bin/nodenv-nvmrc",
    "test": "bats ${CI:+--tap} test",
    "test:docker": "docker run -it --rm -v $PWD:/nodenv-nvmrc node:16 /bin/sh -c 'cd nodenv-nvmrc; npm it'",
    "posttest": "npm run lint",
    "lint": "git ls-files bin etc libexec test */*.*sh | grep -Ev '(semver|JSON|fixtures)' | xargs shellcheck",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "sh-semver": "^1.1.0"
  },
  "devDependencies": {
    "@nodenv/nodenv": "1",
    "bats": "1",
    "shellcheck": "4"
  }
}
