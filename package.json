{
  "name": "@nodenv/nodenv-nvmrc",
  "version": "1.0.6",
  "description": "Activate a nodenv node version from .nvmrc",
  "homepage": "https://github.com/nodenv/nodenv-nvmrc#readme",
  "license": "MIT",
  "author": "Nan Wu",
  "contibutors": [
    "Jason Karns <jason.karns@gmail.com> (http://jasonkarns.com)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nodenv/nodenv-nvmrc.git"
  },
  "bugs": {
    "url": "https://github.com/nodenv/nodenv-nvmrc/issues"
  },
  "directories": {
    "bin": "bin",
    "test": "test"
  },
  "files": [
    "bin",
    "etc"
  ],
  "scripts": {
    "start": "bin/nodenv-nvmrc",
    "test": "bats ${CI:+--tap} test",
    "docker:test": "docker run -it --rm -v $PWD:/nodenv-nvmrc node:16 /bin/sh -c 'cd nodenv-nvmrc; npm it'",
    "postversion": "git push --follow-tags"
  },
  "devDependencies": {
    "@nodenv/nodenv": "^1.6.2",
    "bats": "^1.12.0"
  },
  "dependencies": {
    "sh-semver": "^1.1.0"
  }
}
